root = true

[*]
indent_style = space
indent_size = 4
charset = utf-8
end_of_line = lf
max_line_length = 150
insert_final_newline = true
trim_trailing_whitespace = true

[*.groovy]
max_line_length = off
ij_groovy_label_indent_size = 4

[*.md]
max_line_length = off

[*.yml]
indent_size = 2

[*.json]
indent_size = 2


